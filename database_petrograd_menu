klon.querySelecter(".data_rabatsats").innerHTML =produkt.pris *produkt.rabatsats /100;

klon.querySelecter(".data_rabatsats").src = "/imgs/small"+produkt.billed+"-sm.jpg";

klon.querySelecter(".data_navn").innerHTML=produkt.navn;

 $.getJSON("prfrankild.dk/petrograd/produkt?callback",visProduktlisten";


